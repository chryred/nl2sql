{
  "name": "nl2sql",
  "version": "1.0.0",
  "description": "Natural Language to SQL converter CLI tool",
  "main": "dist/index.js",
  "bin": {
    "nl2sql": "./dist/index.js",
    "nl2sql-mcp": "./dist/mcp/index.js"
  },
  "scripts": {
    "build": "tsc && copyfiles -u 2 src/database/schemas/**/* dist/database/",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "start:mcp": "tsx src/mcp/index.ts",
    "start:mcp:sse": "MCP_TRANSPORT=sse tsx src/mcp/index.ts",
    "docker:build": "docker build -t nl2sql-mcp .",
    "docker:run": "docker compose up -d",
    "docker:stop": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --watch",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage"
  },
  "keywords": [
    "nl2sql",
    "sql",
    "cli",
    "openai",
    "anthropic",
    "natural-language"
  ],
  "author": "",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3",
    "iconv-lite": "^0.7.2",
    "inquirer": "^13.2.2",
    "js-yaml": "^4.1.0",
    "knex": "^3.1.0",
    "mysql2": "^3.16.2",
    "openai": "^6.17.0",
    "ora": "^9.1.0",
    "pg": "^8.18.0",
    "zod": "^4.3.6"
  },
  "metadata": {
    "@devx/mcp-sdk": "latest"
  },
  "optionalDependencies": {
    "oracledb": "^6.7.1"
  },
  "devDependencies": {
    "@types/iconv-lite": "^0.0.1",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
